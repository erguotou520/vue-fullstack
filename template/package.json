{
  "name": "{{name}}",
  "version": "0.0.1",
  "keywords": [
    "vue",
    "vuejs",
    "vuex",
    "vue-router",
    "vue-i18n",
    "vue-resource",
    "fullstack",{{#if_eq mock 'backend'}}
    "mongodb",
    "mongoose",{{/if_eq}}
    "socket.io",
    "element",
    "element-ui"
  ],
  "description": "{{description}}",
  "author": "{{author}}",
  "private": false,
  "scripts": {
    "client": "node {{#if_eq mock 'backend'}}client/{{/if_eq}}build/dev-server.js",{{#if_eq mock 'backend'}}
    "server": "./node_modules/.bin/nodemon --watch server server/app.js",{{/if_eq}}{{#if_eq mock 'mock'}}
    "mock": "./node_modules/.bin/nodemon --watch mock mock/index.js",{{/if_eq}}
    "dev": "npm run client|npm run {{#if_eq mock 'backend'}}server{{/if_eq}}{{#if_eq mock 'mock'}}mock{{/if_eq}}",
    "build": "node {{#if_eq mock 'backend'}}client/{{/if_eq}}build/build.js",
    {{#unless i18n}}"remove:i18n": "node tasks/replaceI18n.js",
    {{/unless}}{{#if_eq mock 'mock'}}"remove:mock": "node tasks/mock.js",
    {{/if_eq}}"lint": "eslint --ext .js,.vue {{#if_eq mock 'backend'}}client/{{/if_eq}}src"
  },
  "dependencies": {
    {{#if_eq mock "backend"}}
    "body-parser": "^1.15.2",
    "composable-middleware": "^0.3.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "crypto": "0.0.3",
    "ejs": "^2.5.2",
    {{/if_eq}}
    "element-ui": "^1.1.3",
    {{#if_eq mock "backend"}}
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    {{/if_eq}}{{#if_eq mock "mock"}}
    "fms": "0.2.0",
    {{/if_eq}}{{#if_eq mock "backend"}}
    "jsonwebtoken": "^7.1.9",
    {{/if_eq}}
    "lodash": "^4.17.2",
    {{#if_eq mock "backend"}}
    "method-override": "^2.3.7",
    {{/if_eq}}{{#if_eq mock "mock"}}
    "mockjs": "^1.0.1-beta3",
    {{/if_eq}}
    "nodemon": "^1.10.2",
    {{#if_eq mock "backend"}}
    "mongoose": "^4.7.0",
    {{/if_eq}}
    "nprogress": "^0.2.0",
    {{#if_eq mock "backend"}}
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    {{/if_eq}}
    "socket.io": "^1.7.1",
    "socket.io-client": "^1.7.1",
    {{#if_eq mock "backend"}}
    "socketio-jwt": "^4.5.0",
    {{/if_eq}}
    "vue": "^2.1.3",
    "vue-i18n": "^5.0.3",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.3",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^4.1.2"
  },
